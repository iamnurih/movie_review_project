{
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/run.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/run.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "run.py": {
      "maxDuration": 30
    }
  }
}
